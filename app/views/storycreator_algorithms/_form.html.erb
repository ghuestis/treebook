<%= simple_form_for(@storycreator_algorithm) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id%>
    <%= f.input :method_name %>
    <%= f.input :path %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
