<%= simple_form_for(@storycreator_algorithm) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :identifier %>
    <%= f.input :server %>
    <%= f.input :database %>
    <%= f.input :tables %>
    <%= f.input :anchor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
